{
    "name": "Breadcrumbs",
    "version": "1.0.0",
    "description": "The definitive personal knowledge management system for the internet",
    "manifest_version": 3,
    "author": "Yash Kaul and Bren Paul",
    "chrome_url_overrides" : {
        "newtab": "page.html"
      },
    "action":{
        "default_popup": "Breadcrumbs",
        "default_title": "Breadcrumbs"
    },
    "permissions": [
        "history",
        "storage",
        "tabs"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "run_at": "document_start",
          "js": ["bubble_content_script.js"]
        }
    ],
    "host_permissions" : ["<all_urls>"]
}